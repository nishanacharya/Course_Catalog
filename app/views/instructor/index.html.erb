<p id="notice"><%= notice %></p>

<h1>Listing Instructors</h1>

<table>
  <thead>
    <tr>
      <th>First Name </th>
      <th>Last Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instuctors.each do |instructor| %>
      <tr>
        <td><%= instructor.First_name %></td>
        <td><%= instructor.Last_name %></td>
        <td><%= instructor.Email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
